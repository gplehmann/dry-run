{
	"manifest_version":"1",
	"type":"WebApp",
	"meta":{
		"name":"Dry Run",
		"version":"1.0.0",
		"minimum-overwolf-version": "0.77.4.0",
		"author":"Greg Lehmann",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Dry Top organization tool"
	},
    "permissions": ["Clipboard"],
	"data": {
	    "start_window":"index",
	    "windows":{
			"index":{
				"file":"Files/index.html",
				"transparent":false,
				"resizable":true,
				"show_in_taskbar": true,
				"size": {
					"width": 600,
					"height": 300
				},
				"start_position": {
					"Top": 100,
					"Left": 300
				}
			}
		},
		"plugins": [
			"Files/npSimpleIOPlugin.dll"
		]
	}
}